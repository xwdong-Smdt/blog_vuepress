#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/blog_vuepress/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_d585ac0b5498a91abb9a41496314ace0/node_modules/vuepress/bin/node_modules:/mnt/d/blog_vuepress/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_d585ac0b5498a91abb9a41496314ace0/node_modules/vuepress/node_modules:/mnt/d/blog_vuepress/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_d585ac0b5498a91abb9a41496314ace0/node_modules:/mnt/d/blog_vuepress/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/blog_vuepress/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_d585ac0b5498a91abb9a41496314ace0/node_modules/vuepress/bin/node_modules:/mnt/d/blog_vuepress/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_d585ac0b5498a91abb9a41496314ace0/node_modules/vuepress/node_modules:/mnt/d/blog_vuepress/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_d585ac0b5498a91abb9a41496314ace0/node_modules:/mnt/d/blog_vuepress/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vuepress@2.0.0-rc.24_@vuepr_d585ac0b5498a91abb9a41496314ace0/node_modules/vuepress/bin/vuepress-webpack.js" "$@"
else
  exec node  "$basedir/../../../../../../vuepress@2.0.0-rc.24_@vuepr_d585ac0b5498a91abb9a41496314ace0/node_modules/vuepress/bin/vuepress-webpack.js" "$@"
fi
